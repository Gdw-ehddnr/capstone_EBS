# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key

# 데이터베이스 설정
DATABASE_URL=postgresql://user:password@localhost:5432/trading_db

# 키움 API 설정
KIWOOM_USER_ID=your_kiwoom_id
KIWOOM_USER_PWD=your_kiwoom_password
KIWOOM_CERT_PWD=your_cert_password

# 웹 대시보드 설정
DASHBOARD_SECRET_KEY=your_secret_key
DASHBOARD_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 에이전트 서버 포트
STRATEGY_AGENT_PORT=8001
RISK_AGENT_PORT=8002
NEWS_AGENT_PORT=8003
TECHNICAL_AGENT_PORT=8004
XAI_AGENT_PORT=8005
TRADING_AGENT_PORT=8006
DASHBOARD_BACKEND_PORT=8000

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/trading_system.log

# 기타 설정
ENVIRONMENT=development  # development, production
DEBUG_MODE=true 